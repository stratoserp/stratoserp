<?php

declare(strict_types=1);

/**
 * @file
 */

use Drupal\se_payment\Entity\Payment;

/**
 * Implements hook_theme().
 */
function se_payment_theme() {
  $theme = [];

  $theme['se_payment'] = [
    'render element' => 'elements',
    'file' => 'se_payment.page.inc',
    'template' => 'se_payment',
  ];

  return $theme;
}

